<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Mobile2You" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimaryMedium</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">@color/colorLightGray</item>
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorSecondaryVariant">@color/colorSecondaryVariant</item>
        <item name="colorOnSecondary">@color/colorPrimaryMedium</item>
    </style>

    <style name="roundimageview" parent="">
        <item name="cornerSizeBottomLeft">30dp</item>
        <item name="cornerSizeBottomRight">30dp</item>
    </style>

    <style name="roundimageviewMedium" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="alpha">0.1</item>
        <item name="android:background">@drawable/shape_line</item>
    </style>

    <style name="ButtonError">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:elevation">4dp</item>
        <item name="android:background">@drawable/shape_button_error</item>
        <item name="android:textColor">@color/colorBlack</item>
    </style>
</resources>